public int finPatch(int input1, int input2, int[][] input3){
    int counter = 0;
    for(int i=0; i<input3.length; i++) {
      int[] input4 = input3[i];
      int out=0;
      for(int j=0; j<input4.length; j++) {
            out= out+input4[j];
      }
      if(out%input2==1) {
        counter = counter+1;
      }
    }
    
    return counter;
  }
